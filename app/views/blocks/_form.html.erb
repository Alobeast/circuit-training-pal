<%= simple_form_for [session, block], html: { class: "form block" } do |f| %>
  <%= form_error_notification(block) %>

  <%= f.input :name, html5: true, input_html: { autofocus: true } %>
  <%= link_to "Cancel", session_path(session), class: "btn btn--light" %>
  <%= f.submit class: "btn btn--secondary" %>
<% end %>
